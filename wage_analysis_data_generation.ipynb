  {
   "cell_type": "code",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2025-08-13T04:38:39.172615Z",
     "start_time": "2025-08-13T04:38:39.139790Z"
    }
   },
   "source": [
    "# Combine and save data\n",
    "data = X.copy()\n",
    "data['log_wage'] = log_wage\n",
    "data.to_csv('DATA/wage_data.csv', index=False)\n",
    "print(\"Data successfully saved to 'wage_data.csv'\")\n",
    "print(f\"File location: {os.path.abspath('DATA/wage_data.csv')}\")\n",
    "print(\"\\nData preview:\")\n",
    "display(data.head())"
